    "order_updated": {
      "type": "object",
      "properties": {
        "server_utc_timestamp": { "type": "string", "pattern": "^[0-9]+$" },
        "market_id": { "type": "string" },
        "symbol": { "type": "string" },
        "tradeable_entity_id": { "type": "string" },
        "side": { "type": "string" },
        "old_order_id": { "type": "string" },
        "new_order_id": { "type": "string" },
        "price": { "type": "string" },
        "quantity": { "type": "string" },
        "utc_timestamp": { "type": "string", "pattern": "^[0-9]+$" }
      }
    }
  }
}
